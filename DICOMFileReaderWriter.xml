<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>Examples</category>
  <title>DICOM File Reader Writer</title>
  <description><![CDATA[]]></description>
  <version>0.1.0.$Revision$</version>
  <documentation-url>http://wiki.slicer.org/slicerWiki</documentation-url>
  <license/>
  <contributor>Alireza Mehrtash (BWH)</contributor>
  <acknowledgements><![CDATA[This work is part of the National Alliance for Medical Image Computing (NAMIC), funded by the National Institutes of Health through the NIH Roadmap for Medical Research, Grant U54 EB005149.]]></acknowledgements>
  <parameters>
    <label>IO</label>
    <description><![CDATA[Input/output parameters]]></description>
    <string>
      <name>inputFile</name>
      <longflag>--inputFile</longflag>
      <description><![CDATA[The input DICOM file.]]></description>
      <label>Input DICOM File</label>
      <default>./</default>
    </string>
    <string>
    <name>outputFile</name>
    <longflag>--outputFile</longflag>
    <description><![CDATA[The output DICOM file.]]></description>
    <label>Output DICOM File</label>
    <default>./</default>
    </string>
    <image>
      <name>outputVolume</name>
      <label>Output Volume</label>
      <channel>output</channel>
      <index>0</index>
      <description><![CDATA[Input volume]]></description>
    </image>
    <string-enumeration>
      <name>ioType</name>
      <description><![CDATA[Type of transform to produce]]></description>
      <label>I/O Type</label>
      <longflag>ioType</longflag>
      <element>GDCM</element>
      <element>DCMTK</element>
      <default>GDCM</default>
    </string-enumeration>
    <string-enumeration>
      <name>imageDimension</name>
      <description><![CDATA[Image Dimension]]></description>
      <label>Image Dimension</label>
      <longflag>imageDimension</longflag>
      <element>2</element>
      <element>3</element>
      <default>2</default>
    </string-enumeration>
  </parameters>
</executable>
